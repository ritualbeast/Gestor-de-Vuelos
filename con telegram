def FechaID(update,context):
    global  idv,fda,fiv
    args=context.args
    if len(args)==0:
       update.message.reply_text("A continuacion escriba /fechaida (000-00-00)") 
    else:
      f=args[0]
      fda=f
      if idv== False:
          update.message.reply_text("A continuacion escriba:/cantidadAsientos (# de asientos que desea)\n ")

