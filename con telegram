def GYEUIO(update,context):
    global idv, vc
    vc.insert(0,Lv[0][0]) #aero
    vc.insert(1,Lv[0][1])#origen
    vc.insert(2,Lv[0][2])#destino
    vc.insert(3,Lv[0][3])#Duracion
    vc.insert(4,Lv[0][4])#precio
    update.message.reply_text("Origen:"+vc[1]+ "\nDestino :" + vc[2]+ "\nPrecio:" +str(vc[4])+"$" + "\nDuracion :"+vc[3]+"\nAerolinea :"+vc[0])
    update.message.reply_text("A continuacion escriba /fechaida (000-00-00)\n Ejemplo /fechaida  24-10-2020") #año -mes-dia
    if idv == True:
        update.message.reply_text("A continuacion escriba /fechavuelta (000-00-00)\n Ejemplo /fechavuelta  24-10-2020")  # año -mes-dia

