def Busqueda(update,context):
    global bv
    args=context.args
    if len(args) == 0:
       bdv(update,context)
    else:
        u=args[0]
        d = u.upper()
        for k in range(len(Lv)):
            if d == Lv[k][1]:
               update.message.reply_text("RESULTADOS DE LA BUSQUEDA  ")
               update.message.reply_text("Origen:"+Lv[k][1]+"\nDestino:"+Lv[k][2]+"\nAerolinea:"+Lv[k][0]+"\nDuracion:"+Lv[k][3]+"\nValor:"+str(Lv[k][4])+"\n")
               bv=True
        if bv==False:
                time.sleep(1)
                update.message.reply_text("NO SE ENCONTRO  NADA")
                bdv(update,context)
                update.message.reply_text("\n- /Menu")

