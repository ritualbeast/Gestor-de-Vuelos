def id(update,context):
    global c ,tp ,n ,ncliente,fda,fiv
    args=context.args
    if len(args)==0:
        update.message.reply_text("Reintente  /cedula 1234567890 ")
    else:
        id = args[0]
        if len(id)==10 and id.isdigit():
            c=id
            if idv == False:
                update.message.reply_text("USTED  HA  REALIZADO  LA  SIGUIENTE  RESERVA   :\n")
                update.message.reply_text("___________________________________________")
                update.message.reply_text("Fecha de Salida : "+ fda)
                update.message.reply_text("Origen:"+vc[1]+ "\nDestino :" + vc[2]+ "\nPrecio:" +str(vc[4])+"$" + "\nDuracion :"+vc[3]+"\nAerolinea :"+vc[0])
            if idv == True:
                pi =vc[4] * 2
                vc[4] = pi
                tp = n *  vc[4] 
                update.message.reply_text("USTED  HA  REALIZADO  LA  SIGUIENTE  RESERVA\n")
                update.message.reply_text("-----------------------------------------------------")
                update.message.reply_text("Fecha de Salida : "+ fda)
                update.message.reply_text("Fecha de Retorno : "+ fiv)
                update.message.reply_text("Origen:"+vc[1]+ "\nDestino :" + vc[2]+ "\nPrecio:" +str(vc[4])+"$" + "\nDuracion :"+vc[3]+"\nAerolinea :"+vc[0])
                

            update.message.reply_text("Precio por Ida y vuelta" + str(pi) + "$")
            update.message.reply_text("Numero de asientos :"+str(n))
            update.message.reply_text("Nombre  :" + ncliente)
            update.message.reply_text("Número de Cedula :"+c)
            update.message.reply_text("Su total a cancelar es de " + str(tp) + "$")
            update.message.reply_text("\n- /Menu")

        else:
             update.message.reply_text("! ingrese cedula valida de 10 digitos !")
             update.message.reply_text("ingrese su cedula con /cedula 0123456789")
             id(update,context,args)


