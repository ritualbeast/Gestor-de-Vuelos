def NCliente(update,context):
    global ncliente
    args=context.args
    if len(args) == 0:
        update.message.reply_text("No puede dejar vacio su Nombre")
        nom(update,context)
    else:
        ncliente=args[0]
        update.message.reply_text("ingrese su cedula con /cedula 0000000000000")

   
